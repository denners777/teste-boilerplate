{
    "name": "denners777/teste-boilerplate",
    "description": "",
    "keywords": [
        "denners777",
        "teste-boilerplate"
    ],
    "homepage": "https://github.com/denners777/teste-boilerplate",
    "license": "MIT License (MIT)",
    "type": "library",
    "authors": [
        {
            "name": "Denner Fernandes",
            "email": "denners777@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Denners777\\TesteBoilerplate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Denners777\\TesteBoilerplate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
